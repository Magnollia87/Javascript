<!DOCTYPE html>
<html>
<body>

<script>
number1 = prompt("Please enter your first number") 
number2 = prompt ("Please enter your second number")

document.write ("Your first number is: " )
document.write (number1)
document.write ("<br>")

document.write ("Your second number is: ")
document.write (number2)

document.write ("<br>")

if (number1 > number2) {

document.write ("your first number was bigger!") 
} 

document.write ("<br>")

if (number2 > number1) {

document.write ("your first number was lower!")
}

document.write ("<br>")

if (number2 == number1) {

document.write ("try once again!")
}

</script>

</body>
</html>
